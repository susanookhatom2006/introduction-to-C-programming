#include <stdio.h>
int findGCD(int a, int b);
int main() {
    int num1, num2, gcd_result;
    printf("Enter two positive integers: ");
    scanf("%d %d", &num1, &num2);
    gcd_result = findGCD(num1, num2);
    printf("The GCD of %d and %d is: %d\n", num1, num2, gcd_result);
    return 0;
}
int findGCD(int a, int b) {
    
    if (b == 0) {
        return a;
    }
    
    else {
        return findGCD(b, a % b);
    }
}
